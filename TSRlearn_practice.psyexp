      <CodeComponent name="exp_settings" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="## set main experimental variables&amp;#10;&amp;#10;# instruction language&amp;#10;language = &quot;english&quot; # &quot;french&quot; or &quot;english&quot;&amp;#10;&amp;#10;# keys&amp;#10;left_key = &quot;left&quot;&amp;#10;center_key = &quot;up&quot;&amp;#10;right_key = &quot;right&quot; &amp;#10;down_key = &quot;down&quot;&amp;#10;&amp;#10;# pos of images (x-coord, y-coord)&amp;#10;left_pos   = (-0.2, 0.1)&amp;#10;right_pos  = (0.2, 0.1)&amp;#10;center_pos = (0.0, 0.1)&amp;#10;prompt_pos = (0, -0.1) &amp;#10;&amp;#10;# Map positions to keys&amp;#10;pos_to_key = {&amp;#10;    right_pos: right_key,&amp;#10;    center_pos: center_key,&amp;#10;    left_pos: left_key&amp;#10;}&amp;#10;# pos of instructions&amp;#10;instruc_pos = (0,0)&amp;#10;&amp;#10;# make coordinates match with position description in excel file &amp;#10;POS = {&amp;#10;    'left':   left_pos,&amp;#10;    'center': center_pos,&amp;#10;    'right':  right_pos&amp;#10;}&amp;#10; &amp;#10; # timings&amp;#10;max_response_time = 3 # [s]&amp;#10; &amp;#10; # feedback&amp;#10;feedback_steps  =  0.06   # steps out of 1 (percent)&amp;#10;rest_jump       =  0.004 # abs distance to target pos&amp;#10;animation_time   = 1.4    # [s]&amp;#10;&amp;#10;# too lso wmessage&amp;#10;too_slow_dur = 0.5&amp;#10;&amp;#10;# breaks (indicate after which learning route you want to break)&amp;#10;break_dur = 30&amp;#10;break_after_route = 10&amp;#10;&amp;#10;# replay breaks during learning&amp;#10;replay_break_dur = 5&amp;#10;break_interval = 13 # after 13 trials because we have 14 image sequences&amp;#10;&amp;#10;# required streak&amp;#10;req_streak = 2&amp;#10;&amp;#10;# break when &quot;req_streak&quot; correct routes in a row&amp;#10;streak_breaks_exp = False&amp;#10;&amp;#10;# duration of response time on retrieval tirals&amp;#10;retr_dur = 5&amp;#10;# how long is retr question presented&amp;#10;retrieval_quest_dur = 3&amp;#10;retr_dur_slider = 5&amp;#10;image_dur_retr = 1.5&amp;#10;fix_dur_retr = 0.5" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="language = &quot;english&quot;;&amp;#10;left_key = &quot;left&quot;;&amp;#10;center_key = &quot;up&quot;;&amp;#10;right_key = &quot;right&quot;;&amp;#10;down_key = &quot;down&quot;;&amp;#10;left_pos = [(- 0.2), 0.1];&amp;#10;right_pos = [0.2, 0.1];&amp;#10;center_pos = [0.0, 0.1];&amp;#10;prompt_pos = [0, (- 0.1)];&amp;#10;pos_to_key = {[right_pos]: right_key, [center_pos]: center_key, [left_pos]: left_key};&amp;#10;instruc_pos = [0, 0];&amp;#10;POS = {&quot;left&quot;: left_pos, &quot;center&quot;: center_pos, &quot;right&quot;: right_pos};&amp;#10;max_response_time = 3;&amp;#10;feedback_steps = 0.06;&amp;#10;rest_jump = 0.004;&amp;#10;animation_time = 1.4;&amp;#10;too_slow_dur = 0.5;&amp;#10;break_dur = 30;&amp;#10;break_after_route = 10;&amp;#10;replay_break_dur = 5;&amp;#10;break_interval = 13;&amp;#10;req_streak = 2;&amp;#10;streak_breaks_exp = false;&amp;#10;retr_dur = 5;&amp;#10;retrieval_quest_dur = 3;&amp;#10;retr_dur_slider = 5;&amp;#10;image_dur_retr = 1.5;&amp;#10;fix_dur_retr = 0.5;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
